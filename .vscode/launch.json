  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "run debug",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "debug"
      ],
      "port": 9229,
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "csv => csv",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "transfer",
        "${workspaceFolder}/test/data/foofile.csv",
        "${workspaceFolder}/test/output/csv_foofile.csv"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "json => json",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "transfer",
        "${workspaceFolder}/test/data/foofile.json",
        "${workspaceFolder}/test/output/json_foofile.json"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "csv => json",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "transfer",
        "${workspaceFolder}/test/data/foo.csv",
        "${workspaceFolder}/test/output/csv_foofile.json"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "json => transform => csv",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "transfer",
        "${workspaceFolder}/test/data/foofile.json",
        "${workspaceFolder}/test/output/json_foofile.csv",
        "${workspaceFolder}/test/data/transforms.json"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "etl-1: json => transform => csv",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "transfer",
        "${workspaceFolder}/test/config/config-etl-1.json"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "etl-2: json => transforms => elasticsearch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "transfer",
        "${workspaceFolder}/test/config/config-etl-2.json"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "etl-3: rest => inject => csv",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "transfer",
        "${workspaceFolder}/test/config/config-etl-3.json"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "config-scan-1.json",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "scan",
        "${workspaceFolder}/test/config/config-scan-1.json"
      ],
      "outputCapture": "std"
    }
  ]
}
