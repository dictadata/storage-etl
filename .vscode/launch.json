  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "run debug",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "debug"
      ],
      "env": {"NODE_ENV": "development", "LOG_LEVEL": "verbose"},
      "port": 9229,
      "outputCapture": "std"
    },

    {
      "type": "node",
      "request": "launch",
      "name": "fs list",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "list",
        "-c",
        "${workspaceFolder}/test/fs/list.json"
      ],
      "env": {"NODE_ENV": "development", "LOG_LEVEL": "verbose"},
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "fs codify",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "scan",
        "-c",
        "${workspaceFolder}/test/fs/codify.json",
        "csv-source"
      ],
      "env": {"NODE_ENV": "development", "LOG_LEVEL": "verbose"},
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "fs scan",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "scan",
        "-c",
        "${workspaceFolder}/test/fs/scan.json"
      ],
      "env": {"NODE_ENV": "development", "LOG_LEVEL": "verbose"},
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "fs transfer csv > output",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "transfer",
        "-c",
        "${workspaceFolder}/test/fs/transfer.json",
        "csv-source",
        "csv-destination"
      ],
      "env": {"NODE_ENV": "development", "LOG_LEVEL": "verbose"},
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "fs transfer json > output",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "transfer",
        "-c",
        "${workspaceFolder}/test/fs/transfer.json",
        "json-source",
        "json-destination"
      ],
      "env": {"NODE_ENV": "development", "LOG_LEVEL": "verbose"},
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "fs transfer csv > json",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "transfer",
        "-c",
        "${workspaceFolder}/test/fs/transfer.json",
        "csv-source",
        "json2-destination"
      ],
      "env": {"NODE_ENV": "development", "LOG_LEVEL": "verbose"},
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "fs transfer json > csv",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "transfer",
        "-c",
        "${workspaceFolder}/test/fs/transfer.json",
        "json-source",
        "csv2-destination"
      ],
      "env": {"NODE_ENV": "development", "LOG_LEVEL": "verbose"},
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "fs transform csv > json",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "transfer",
        "-c",
        "${workspaceFolder}/test/fs/transform.json",
        "csv-source",
        "json-destination"
      ],
      "env": {"NODE_ENV": "development", "LOG_LEVEL": "verbose"},
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "fs transform json > csv",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "transfer",
        "-c",
        "${workspaceFolder}/test/fs/transform.json",
        "json-source",
        "csv-destination"
      ],
      "env": {"NODE_ENV": "development", "LOG_LEVEL": "verbose"},
      "outputCapture": "std"
    }


  ]
}
