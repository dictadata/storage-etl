  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "run debug",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "debug"
      ],
      "port": 9229,
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "csv => csv",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "${workspaceFolder}/test/data/input.csv",
        "${workspaceFolder}/test/output/output.csv"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "json => json",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "${workspaceFolder}/test/data/input.json",
        "${workspaceFolder}/test/output/output.json"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "csv => json",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "${workspaceFolder}/test/data/foo.csv",
        "${workspaceFolder}/test/output/transfer.json"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "json => transform => csv",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "${workspaceFolder}/test/data/input.json",
        "${workspaceFolder}/test/output/transform.csv",
        "${workspaceFolder}/test/data/transforms.json"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "etl-1: json => transform => csv",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "${workspaceFolder}/test/config/config-etl-1.json"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "etl-2: json => transforms => elasticsearch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "${workspaceFolder}/test/config/config-etl-2.json"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "etl-3: rest => inject => csv",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "${workspaceFolder}/test/config/config-etl-3.json"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Franchise API",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "${workspaceFolder}/test/config/foodusage.json"
      ],
      "outputCapture": "std"
    }
  ]
}
