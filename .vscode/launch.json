  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "copy csv",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "test/data/input.csv",
        "test/output/output.csv"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "copy json",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "test/data/input.json",
        "test/output/output.json"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "convert",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "test/data/input.csv",
        "test/output/output.json"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "transform",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/storage-etl.js",
      "args": [
        "test/data/input.json",
        "test/output/output.csv",
        "test/data/transforms.json" ],
      "outputCapture": "std"
    }
  ]
}
