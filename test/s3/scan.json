{
  "storage": {
    "source": {
      "smt": "json|S3:dictadata.org/test/data/|*.json|*",
      "options": {
        "s3": {
          "aws_profile": "dictadata"
        },
        "reader": {
          "max_read": 10
        },
        "list": {
          "schema": "foofile_*.json",
          "recursive": false
        },
        "codify": {
          "encoding": {},
          "statistics": true
        }
      },
      "outputFile": "./test/output/s3_scan.json"
    }
  }
}
