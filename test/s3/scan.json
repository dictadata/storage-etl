{
  "source": {
    "smt": "jsonl|S3:dictadata.org/test/|*.json|*",
    "options": {
      "s3": {
        "aws_profile": ""
      },
      "reader": {
        "max_read": 8
      }
    }
  },
  "list": {
    "filespec": "*.log.gz",
    "recursive": true
  },
  "codify": {
    "notation": "dot",
    "statistics": {
      "count": true,
      "minimum": true,
      "maximum": true,
      "length": true,
      "nulls": true,
      "dateField": "at"
    }
  },
  "outputFile": "./test/output/scan_encoding.json"
}
