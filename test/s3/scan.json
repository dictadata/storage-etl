{
  "scan": {
    "origin": {
      "smt": "json|S3:dictadata.org/test/data/|*|*",
      "options": {
        "s3": {
          "aws_profile": "dictadata"
        },
        "max_read": 100,
        "schema": "foofile_*.json",
        "recursive": false,
        "statistics": true
      }
    },
    "terminal": {
      "output": "./test/output/s3_scan.json"
    }
  }
}