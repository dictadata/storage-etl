{
  "split": {
    "origin": {
      "smt": "json|./test/data/|db_schema.json|*"
    },
    "terminal": {
      "smt": "split|*|*|*",
      "options": {
        "splitOn": "TABLE_NAME",
        "encoding": "./test/data/db_schema_encoding.json"
      },
      "tract": {
        "terminal": {
          "smt": "json|./output/DB/|db_*|*",
          "options": {}
        }
      }
    }
  }
}