{
  "source": {
    "smt": "jsonl|S3:bucket|*|*",
    "options": {
      "aws": {
        "accessKeyId": "",
        "secretAccessKey": "",
        "region": ""
      },
      "max_read": 8
    }
  },
  "scan": {
    "filespec": "*.log.gz",
    "recursive": false
  },
  "codify": {
    "notation": "dot",
    "statistics": {
      "count": true,
      "minimum": true,
      "maximum": true,
      "length": true,
      "nulls": true,
      "dateField": "at"
    }
  },
  "output": "./test/output/encoding.json"
}
