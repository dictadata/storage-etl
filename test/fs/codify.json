{
  "codify": {
    "origin": {
      "smt": "json|data/test/|foofile.json|*",
      "options": {
        "max_read": 100
      }
    },
    "terminal": {
      "output": "./data/output/fs/codify.json"
    }
  },
  "types_only": {
    "origin": {
      "smt": "json|data/test/|foofile.json|*",
      "options": {
        "max_read": 100,
        "encoding_format": "types_only"
      }
    },
    "terminal": {
      "output": "./data/output/fs/codify_types.json"
    }
  },
  "all": {
    "origin": {
      "smt": "json|data/test/|foofile.json|*",
      "options": {
        "max_read": 100,
        "encoding_format": "all"
      }
    },
    "terminal": {
      "output": "./data/output/fs/codify_all.json"
    }
  }

}