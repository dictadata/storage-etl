{
  "codify": {
    "action": "codify",
    "origin": {
      "smt": "json|./test/data/|foofile.json|*",
      "options": {
        "max_read": 100
      }
    },
    "terminal": {
      "output": "./test/data/output/fs/codify.json"
    }
  },
  "types_only": {
    "action": "codify",
    "origin": {
      "smt": "json|./test/data/|foofile.json|*",
      "options": {
        "max_read": 100,
        "encoding_format": "types_only"
      }
    },
    "terminal": {
      "output": "./test/data/output/fs/codify_types.json"
    }
  },
  "all_fields": {
    "action": "codify",
    "origin": {
      "smt": "json|./test/data/|foofile.json|*",
      "options": {
        "max_read": 100,
        "encoding_format": "all"
      }
    },
    "terminal": {
      "output": "./test/data/output/fs/codify_all.json"
    }
  }

}