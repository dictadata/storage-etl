{
  "storage": {
    "csv-source": {
      "smt": "csv|./test/data/|foofile.csv|*",
      "options": {}
    },
    "json-source": {
      "smt": "json|./test/data/|foofile.json|*",
      "options": {}
    },
    "csv-destination": {
      "smt": {
        "model": "csv",
        "locus": "./test/output/",
        "schema": "fs_transform.csv",
        "key": "*"
      },
      "options": {},
      "create": true
    },
    "json-destination": {
      "smt": {
        "model": "json",
        "locus": "./test/output/",
        "schema": "fs_transform.json",
        "key": "*"
      },
      "options": {},
      "create": true
    }
  },
  "transforms": {
    "transform": {
      "filter": {
        "match": {
          "Bar": "row"
        },
        "drop": {
          "Baz": 456
        }
      },
      "select": {
        "inject_before": {
          "Fie": "where's fum?"
        },
        "fields": {
          "Foo": "foo",
          "Bar": "bar",
          "Baz": "baz",
          "Fobe": "fobe",
          "Dt Test": "dt_test",
          "enabled": "enabled",
          "subObj1.state": "state",
          "subObj2.subsub.izze": "izze"
        }
      }
    }
  }
}