{
  "csv-json": {
    "origin": {
      "smt": "csv|./data/test/|foofile.csv|*",
      "options": {
        "header": true
      }
    },
    "transform": {
      "filter": {
        "match": {
          "Bar": "row"
        },
        "drop": {
          "Baz": 456
        }
      },
      "select": {
        "inject_before": {
          "fie": "where's fum?"
        },
        "inject_after": {
          "state": "unknown"
        },
        "fields": {
          "Foo": "foo",
          "Bar": "bar",
          "Baz": "baz",
          "Fobe": "fobe",
          "Dt Test": "dt_test"
        },
        "remove": ["fobe"]
      }
    },
    "terminal": {
      "smt": {
        "model": "json",
        "locus": "./data/output/fs/",
        "schema": "transform.json",
        "key": "*"
      },
      "options": {
        "encoding": "./data/test/foo_encoding_t.json"
      }
    }
  },
  "json-csv": {
    "origin": {
      "smt": "json|./data/test/|foofile.json|*"
    },
    "transform": {
      "filter": {
        "match": {
          "Bar": "row"
        },
        "drop": {
          "Baz": 456
        }
      },
      "select": {
        "inject_before": {
          "fie": "where's fum?"
        },
        "fields": {
          "Foo": "foo",
          "Bar": "bar",
          "Baz": "baz",
          "Fobe": "fobe",
          "Dt Test": "dt_test",
          "subObj1.state": "state"
        },
        "remove": ["fobe"]
      }
    },
    "terminal": {
      "smt": {
        "model": "csv",
        "locus": "./data/output/fs/",
        "schema": "transform.csv",
        "key": "*"
      },
      "options": {
        "header": true,
        "encoding": "./data/test/foo_encoding_t.json"
      }
    }
  }
}