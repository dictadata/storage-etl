{
  "csv-sheet": {
    "origin": {
      "smt": "csv|./data/test/|foofile.csv|*",
      "options": {
        "header": true
      }
    },
    "terminal": {
      "smt": {
        "model": "xlsx",
        "locus": "./data/output/xlsx/foofile.xlsx",
        "schema": "fooee",
