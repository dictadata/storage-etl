{
  "csv-table": {
    "origin": {
      "smt": "csv|./test/data/|foofile.csv|*",
      "options": {
        "header": true
      }
    },
    "terminal": {
      "smt": {
        "model": "oracledb",
        "locus": "connectString=localhost/xepdb1;user=dicta;password=data",
        "schema": "foo_schema_etl",
        "key": "*"
      }
    }
  },
  "table-csv": {
    "origin": {
      "smt": "oracledb|connectString=localhost/xepdb1;user=dicta;password=data|foo_schema|*"
    },
    "terminal": {
      "smt": "csv|./test/data/output/oracledb/|foofile.csv|*",
      "options": {
        "header": true
      }
    }
  },
  "bulkload": {
    "origin": {
      "smt": "csv|./test/data/|timeseries.csv|*",
      "options": {
        "encoding": {
          "time": "date",
          "temp": "number"
        }
      }
    },
    "terminal": {
      "smt": {
        "model": "oracledb",
        "locus": "connectString=localhost/xepdb1;user=dicta;password=data",
        "schema": "timeseries",
        "key": "*"
      },
      "options": {
        "bulkLoad": true
      }
    }
  }
}