{
  "ftp-upload": {
    "origin": {
      "options": {
        "uploads": "./data/test/*.json"
      }
    },
    "terminal": {
      "smt": "*|ftp://dicta:data@localhost/data/output/uploads/|*|*"
    }
  },
  "ftp-download": {
    "origin": {
      "smt": "json|ftp://dicta:data@localhost/data/test/|*.json|*",
      "options": {}
    },
    "terminal": {
      "options": {
        "downloads": "./data/output/ftp/downloads/"
      }
    }
  }
}