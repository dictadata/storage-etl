{
  "ftp-upload": {
    "origin": {
      "options": {
        "uploads": "./test/data/*.json"
      }
    },
    "terminal": {
      "smt": "*|ftp://dicta:data@localhost/output/uploads/|*|*"
    }
  },
  "ftp-download": {
    "origin": {
      "smt": "json|ftp://dicta:data@localhost/test/data/|*.json|*",
      "options": {}
    },
    "terminal": {
      "options": {
        "downloads": "./output/downloads/"
      }
    }
  }
}