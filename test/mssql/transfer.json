{
  "csv-table": {
    "origin": {
      "smt": "csv|./data/test/|foofile.csv|*",
      "options": {
        "header": true
      }
    },
    "terminal": {
      "smt": {
        "model": "mssql",
        "locus": "server=localhost;username=dicta;password=data;database=storage_node",
        "schema": "foo_schema_etl",
        "key": "*"
      }
    }
  },
  "table-csv": {
    "origin": {
      "smt": "mssql|server=localhost;username=dicta;password=data;database=storage_node|foo_schema|*"
    },
    "terminal": {
      "smt": "csv|./data/output/mssql/|foofile.csv|*",
      "options": {
        "header": true
      }
    }
  },
  "bulkload": {
    "origin": {
      "smt": "csv|./data/test/|timeseries.csv|*",
      "options": {
        "encoding": {
          "time": "date",
          "temp": "number"
        }
      }
    },
    "terminal": {
      "smt": {
        "model": "mssql",
        "locus": "server=localhost;username=dicta;password=data;database=storage_node",
        "schema": "timeseries",
        "key": "*"
      },
      "options": {
        "bulkLoad": true
      }
    }
  }
}